<!DOCTYPE html><%= react_component(
                     "Root",
                     {
                       location: request.fullpath,
                       INITIAL_STATE: @INITIAL_STATE,
                       applicationSrc: asset_pack_path("application.js"),
                       applicationCSSSrc: asset_pack_path("application.css")
                     },
                     prerender: :static
                   )
                    .sub(/<[^>]*>/,"")
                    .chomp("</div>")
                    .html_safe %>