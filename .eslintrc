{
  "extends": [
    "plugin:react-app/recommended"
  ],
  "rules": {
    "curly": [2, "all"],
    "no-unused-vars": ["error", { "vars": "all", "args": "all" }],
    "one-var": ["error", "never"],
    "quotes": [
      "error",
      "single",
      {
        "avoidEscape": true,
        "allowTemplateLiterals": true
      }
    ],
    "semi": "error"
  }
}
